{
  "easycom": {
    "^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
  },
  "pages": [
    //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/tabbar/index/index",
      "style": {
        "navigationBarTitleText": "首页"
      }
    },
	{
	  "path": "pages/tabbar/index/framework",
	  "style": {
	    "navigationBarTitleText": "框架"
	  }
	},
	{
	  "path": "pages/tabbar/index/youku",
	  "style": {
	    "navigationBarTitleText": "优酷视频"
	  }
	},
    {
      "path": "pages/tabbar/center/center",
      "style": {
        "navigationBarTitleText": "中心",
        "enablePullDownRefresh": false
      }
    },
	{
	  "path": "pages/tabbar/center/detail",
	  "style": {
	    "navigationBarTitleText": "详情",
	    "enablePullDownRefresh": false
	  }
	},
    {
      "path": "pages/tabbar/user/user",
      "style": {
        "navigationBarTitleText": "我的"
      }
    },
    {
      "path": "pages/user/login/login",
      "style": {
        "navigationBarTitleText": "登录",
        "enablePullDownRefresh": false
        // "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/user/login/reg",
      "style": {
        "navigationBarTitleText": "",
        "navigationBarBackgroundColor": "#FFFFFF"
      }
    },
    {
      "path": "pages/user/login/forget",
      "style": {
        "navigationBarTitleText": "忘记密码",
        "navigationBarBackgroundColor": "#FFFFFF"
      }
    },
    {
      "path": "pages/user/login/agreement",
      "style": {
        "navigationBarTitleText": "用户协议"
      }
    },
    {
      "path": "pages/wxDemo/wxDemo",
      "style": {
        "navigationBarTitleText": "微信功能体验",
        "enablePullDownRefresh": false
      }
    }
  ],
  "globalStyle": {
    // #ifdef H5
    // "navigationStyle": "custom",
    // #endif
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "tabBar": {
    "color": "#7A7E83",
    "selectedColor": "#e97516",
    "borderStyle": "white",
    "backgroundColor": "#fff",
    "list": [
      {
        "pagePath": "pages/tabbar/index/index",
        "iconPath": "static/tabbar/home.png",
        "selectedIconPath": "static/tabbar/home-active.png",
        "text": "首页"
      },
      {
        "pagePath": "pages/tabbar/center/center",
        "iconPath": "static/tabbar/home.png",
        "selectedIconPath": "static/tabbar/home-active.png",
        "text": "中心"
      },
      {
        "pagePath": "pages/tabbar/user/user",
        "iconPath": "static/tabbar/user.png",
        "selectedIconPath": "static/tabbar/user-active.png",
        "text": "我的"
      }
    ]
  }
}
